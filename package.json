{
  "name": "trading-operator-bot",
  "version": "1.0.0",
  "description": "A trading bot operator to create, delete or edit orders through an API",
  "private": true,
  "license": "UNLICENSED",
  "author": "Edixon Piña",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc",
    "dev": "yarn run env ts-node src",
    "prod": "ts-node src",
    "env": "cross-env NODE_ENV=development",
    "dist": "yarn run clean && yarn run build && node dist/src",
    "start": "nodemon"
  },
  "keywords": [
